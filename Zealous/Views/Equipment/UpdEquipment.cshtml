
@{
    ViewBag.Title = "UpdEquipment";
}

<h2>Update Equipment</h2>

<center>
    <form>
        <fieldset>
            <legend>Update Equipment</legend>
            <br><br>
            Name of item : <input type="text" name="name"><br><br>
            No. of item : <input type="number" name="number"><br><br>
        </fieldset>
    </form>
    @Html.ActionLink("Submit", "Upd", new { controller = "Equipment" }, new { @class = "btn btn-primary" })
</center>
