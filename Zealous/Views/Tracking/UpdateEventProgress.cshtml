@model Zealous.Models.ProgressDetail
@{
    ViewBag.Title = "UpdateEventProgress";
}

<h2>Update Event Progress</h2>

<div class="form-group">
    @Html.LabelFor(model => model.EventName, new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.TextBoxFor(model => model.EventStatus, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
    </div>
</div>

<input type="submit" value="Save" />